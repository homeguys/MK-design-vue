  "main": "index.js",
  "type": "module",
  "scripts": {
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "build": "node --require=suppress-experimental-warnings --loader @esbuild-kit/esm-loader scripts/build.ts",
    "publish": "node --require=suppress-experimental-warnings --loader @esbuild-kit/esm-loader scripts/publish.ts",
    "genComp": "node --require=suppress-experimental-warnings --loader @esbuild-kit/esm-loader scripts/gen-comp.ts",
    "lint": "pnpm run lint:style && pnpm run lint:js",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .eslintignore",
    "lint:style": "stylelint **/*.{css,scss,vue} --cache --cache-location node_modules/.cache/stylelint/ --fix",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@ant-design/icons-vue": "^7.0.1",
