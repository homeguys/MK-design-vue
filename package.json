    "ant-design-vue": "^4.0.7",
    "chalk": "^4.1.2",
    "dayjs": "^1.11.10",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-vue": "^9.15.1",
    "execa": "^5.1.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^10.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-sass": "^5.1.0",
    "husky": "^8.0.0",
    "inquirer": "^8.2.6",
